gen_ar1 <- function(N = 150, phi1 = 0.8, sigma2 = 1){
a <- rnorm(N,0,sigma2)
y <- as.numeric(0)
y[1] <- a[1]
for(i in 2:N){
y[i] = phi1*y[i-1]+a[i]
}
return(y)
}
realizaciones3 <- replicate(n = 10 ,gen_ar1(N=150,phi1=1.3,sigma2=1), simplify = "array" )
ts.plot(realizaciones3,col=1:10)
